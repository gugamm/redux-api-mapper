{
  "name": "redux-api-mapper",
  "version": "3.6.1",
  "description": "A redux api mapper",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register  tests/**/*.js",
    "testw": "mocha --watch --require babel-polyfill --compilers js:babel-register  tests/**/*.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel src -d dist --ignore node_modules"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gugamm/redux-api-mapper"
  },
  "author": "gugamm",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7"
  }
}
